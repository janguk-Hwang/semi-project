<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.AccountMapper">
    <insert id="insertAccount" parameterType="AccountDto">
        insert into account(account_id,account,name,bank,member_id) values(seq_account.nextval,#{account},#{name},
        #{bank},#{member_id})
    </insert>
</mapper>